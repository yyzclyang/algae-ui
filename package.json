{
  "name": "algae-ui",
  "version": "0.2.1",
  "description": "A Component Library for React.js.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config scripts/webpack.config.dev.js",
    "build": "npm run compile && npm run dist",
    "compile": "gulp --gulpfile scripts/gulpfile.js",
    "dist": "rimraf dist && cross-env NODE_ENV=production webpack --config scripts/webpack.config.dist.js",
    "docs": "rimraf docs && cross-env NODE_ENV=production webpack --config scripts/webpack.config.docs.js",
    "lint:lib": "eslint --ext .ts,.tsx,.js,.jsx  components",
    "lint:site": "eslint --ext .ts,.tsx,.js,.jsx  site",
    "test": "jest",
    "ci": "jest --coverage"
  },
  "files": [
    "es",
    "lib",
    "dist",
    "LICENSE"
  ],
  "sideEffects": [
    "dist/*",
    "lib/*",
    "es/*",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyzclyang/algae-ui.git"
  },
  "keywords": [
    "react",
    "UI",
    "components"
  ],
